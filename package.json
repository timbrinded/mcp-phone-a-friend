{
  "name": "model-agency",
  "version": "1.0.2",
  "type": "module",
  "packageManager": "bun@1.1.0",
  "license": "MIT",
  "description": "MCP server providing an agency of AI models for expert consultation and idiomatic code generation",
  "repository": {
    "type": "git",
    "url": "https://github.com/timbrinded/model-agency-mcp"
  },
  "bin": "./dist/run.js",
  "zshy": {
    "bin": "./src/run.ts",
    "exports": "./src/index.ts",
    "cjs": false
  },
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  },
  "scripts": {
    "start": "bun run src/run.ts",
    "build": "zshy",
    "dev": "bun run --hot src/run.ts",
    "check": "bun --bun tsc --noEmit",
    "test": "bun test",
    "test:watch": "bun test --watch",
    "test:server": "bun test test/server.test.ts",
    "test:tools": "bun test test/tools.test.ts",
    "test:integration": "bun test test/integration.test.ts"
  },
  "dependencies": {
    "@ai-sdk/anthropic": "^2.0.1",
    "@ai-sdk/google": "^2.0.4",
    "@ai-sdk/openai": "^2.0.10",
    "@ai-sdk/xai": "^2.0.5",
    "@modelcontextprotocol/sdk": "^1.17.2",
    "ai": "^5.0.10",
    "openai": "^5.12.2",
    "p-limit": "^6.2.0",
    "zod": "^4.0.17"
  },
  "devDependencies": {
    "@types/bun": "^1.2.20",
    "typescript": "^5.6.2",
    "zshy": "^0.3.5"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "private": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  }
}
